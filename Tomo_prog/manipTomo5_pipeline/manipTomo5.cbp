<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="manipTomo5" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="bin/Debug/manipTomo5" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Debug/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Option parameters="-ni 400 -vfleur 4.15 2.9 -voffset -0.15 -0.2" />
				<Compiler>
					<Add option="-g" />
					<Add directory="/usr/include/opencv" />
				</Compiler>
			</Target>
			<Target title="Release">
				<Option output="bin/Release/manipTomo5" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Release/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
					<Add option="-static" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-fexceptions -std=c++11" />
			<Add option="-D_UNIX_ -D_LINUX_" />
			<Add directory="include" />
			<Add directory="include/LabJack" />
			<Add directory="include/john" />
			<Add directory="include/pleora" />
		</Compiler>
		<Linker>
			<Add option="`pkg-config --cflags --libs opencv` -lpthread -lm -lboost_system -lboost_filesystem -lboost_chrono  -lPvBase -lPvDevice -lPvBuffer -lPvGenICam" />
			<Add option="-lPvStream -lPvTransmitter -lPvVirtualDevice" />
			<Add option="-lPvAppUtils   -lPvPersistence -lPvSerial -lPvGUI -lSimpleImagingLib -lboost_thread" />
			<Add library="/opt/lib_tomo/labjack/libLjack.so" />
			<Add library="/opt/lib_tomo/labjack/liblabjackusb.so" />
			<Add library="/opt/lib_tomo/labjack/liblabjackusbcpp.so" />
			<Add library="/opt/lib_tomo/labjack/libmsleep.so" />
			<Add directory="lib" />
			<Add directory="lib/genicam/bin/Linux64_x64" />
		</Linker>
		<Unit filename="fonctions.cpp" />
		<Unit filename="fonctions.h" />
		<Unit filename="main.cpp" />
		<Extensions>
			<envvars />
			<code_completion />
			<lib_finder disable_auto="1" />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
